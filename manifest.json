{
  "name": "changeTabs",
  "version": "0.1",
  "manifest_version": 2,

  "description": "change Tabs.",

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "changeTabs",
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "windows": "Ctrl+Shift+K",
        "mac": "Ctrl+Shift+K",
        "linux": "Ctrl+Shift+K"
      },
      "description": "Invoke browser action."
    }
  }
}
